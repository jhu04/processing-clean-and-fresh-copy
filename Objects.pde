void draw()
{ 
  // if start button has not been pressed
  if (!start)
  {
    background(255, 255, 255);
    g.display();
    return;
  }
  
  if (curSquare != null)
  {
    curSquare.display();
  }

  // one step of bfs
  for (int steps = 0; steps < stepSize; steps++)
  {
    if (bfsQueue.size() > 0)
    {
      // get first node
      curSquare = bfsQueue.get(0);
      bfsQueue.remove(0);
      
      // get first path
      curPath = pathQueue.get(0);
      pathQueue.remove(0);
      
      // get next node and path until valid
      while (curSquare.visited && bfsQueue.size() > 0)
      {
        // get node
        curSquare = bfsQueue.get(0);
        bfsQueue.remove(0);
        
        // get path
        curPath = pathQueue.get(0);
        pathQueue.remove(0);
      }
      
      // if node is finish
      if (curSquare.finish)
      {
        // mark out path
        int curR = startR, curC = startC;
        g.grid[curR][curC].finish = true;
        
        while (curPath.size() > 0)
        {
          curR += dr[curPath.get(0)];
          curC += dc[curPath.get(0)];
          curPath.remove(0);
          
          g.grid[curR][curC].finish = true;
        }
        
        // display path
        g.display();
        
        // end all actions
        while (bfsQueue.size() > 0)
        {
          bfsQueue.remove(0);
        }
        
        return;
      }
      
      // set node as current node
      curSquare.cur = true;
      
      // set node as visited
      curSquare.visited = true;
      
      // display update
      g.grid[curSquare.r][curSquare.c].display();
      
      // slow down steps
      delay(delayAmount);
      
      // loop through next nodes
      for (int i = 0; i < dr.length; i++)
      {
        // check if should push back into queue
        if (g.valid(curSquare.r + dr[i], curSquare.c + dc[i]))
        {
          // get next node
          nextSquare = g.grid[curSquare.r + dr[i]][curSquare.c + dc[i]];
          nextSquare.len = curSquare.len + 1;
          
          // add next node to queue
          bfsQueue.add(nextSquare);
          
          // make next path
          nextPath = new ArrayList<Integer>();
          for (int j = 0; j < curPath.size(); j++)
          {
            nextPath.add(curPath.get(j));
          }
          nextPath.add(i);
          
          // add next path to queue
          pathQueue.add(nextPath);
        }
      }
      
      g.grid[curSquare.r][curSquare.c].cur = false;
      g.grid[curSquare.r][curSquare.c].display();
    }
  }
  
  // show pointer
  curSquare.cur = true;
  curSquare.display();
  curSquare.cur = false;
}

class Grid
{
  int numR, numC;
  GridSquare[][] grid;
  
  Grid(int numR, int numC, boolean[][] walls)
  {
    this.numR = numR;
    this.numC = numC;
    grid = new GridSquare[numR][numC];
    for (int i = 0; i < numR; i++)
    {
      for (int j = 0; j < numC; j++)
      {
        grid[i][j] = new GridSquare(i, j, gridSquareSize, gridSquareSize, false, false, false);
        if (walls.length > i && walls[0].length > j)
        {
          grid[i][j] = new GridSquare(i, j, gridSquareSize, gridSquareSize, false, walls[i][j], false);
        }
      }
    }
  }
  
  /*
   * checks if can go to a certain node
  */
  boolean valid(GridSquare square)
  {
    if (0 <= square.r && square.r < numR && 0 <= square.c && square.c < numC && square.wall)
    {
      return false;
    }
    
    return 0 <= square.r && square.r < numR && 0 <= square.c && square.c < numC && 
      !grid[square.r][square.c].visited;
  }
  
  /*
   * checks if can go to node at (r,c)
  */
  boolean valid(int r, int c)
  {
    if (0 <= r && r < numR && 0 <= c && c < numC && grid[r][c].wall)
    {
      return false;
    }
    
    return 0 <= r && r < numR && 0 <= c && c < numC && 
      !grid[r][c].visited;
  }
  
  /*
   * displays grid
   */
  void display()
  {
    for (int i = 0; i < numR; i++)
    {
      for (int j = 0; j < numC; j++)
      {
        grid[i][j].display();
      }
    }
  }
}

class GridSquare
{
  int r, c, w, h, len;
  boolean visited, cur, wall, finish;
 
  GridSquare(int r, int c, int w, int h, boolean visited, boolean wall, boolean finish)
  {
    this.r = r;
    this.c = c;
    this.w = w;
    this.h = h;
    this.len = -1;
    this.cur = false;
    this.visited = visited;
    this.wall = wall;
    this.finish = finish;
  }
  
  void display()
  {
    rectMode(CORNERS);
    stroke(0);
    
    fill(255, 255, 255);
    
    if (visited)
    {
      double multiplier = Math.sqrt(dr[0] * dr[0] + dc[0] * dc[0]) / 2;
      fill((int)(255 * multiplier * len / gridSize), 0, (int)(255 - 255 * multiplier * len / gridSize));
    }
    
    if (cur)
    {
      fill(0, 255, 0);
    }
    
    if (wall)
    {
      fill(0, 0, 0);
    }
    
    if (finish)
    {
      fill(255, 255, 0);
    }
    
    rect(r * w, c * h, (r + 1) * w, (c + 1) * h);
  }
}
